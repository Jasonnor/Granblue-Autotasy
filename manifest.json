{
	"manifest_version": 2,
	"name": "Granblue Autotasy",
	"description": "Granblue Fantasy Auto Coopraid Fight",
	"version": "0.7",
	"icons": {
		"16": "src/icon/icon16.png",
		"48": "src/icon/icon48.png",
		"128": "src/icon/icon128.png"
	},
	"browser_action": {
		"default_icon": "src/icon/on.png"
	},
	"content_scripts": [{
		"matches": ["*://gbf.game.mbga.jp/*"],
		"js": ["js/jquery-2.2.0.min.js", "js/auto.js"]
	}],
	"web_accessible_resources": ["js/coopraid.js"],
	"background": {
		"scripts": ["js/background.js"],
		"persistent": false
	},
	"options_page": "options.html",
	"permissions": [
		"tabs",
		"activeTab",
		"storage"
	]
}
